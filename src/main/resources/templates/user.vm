<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="author" content="TezApp Company">


    <meta http-equiv="cache-control" content="max-age=0" />
    <meta http-equiv="cache-control" content="no-cache" />
    <meta http-equiv="expires" content="0" />
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT" />
    <meta http-equiv="pragma" content="no-cache" />


    <title>A. Nicat</title>
    <link href="css/main.css" rel="stylesheet">
    <link href="css/slider.css" rel="stylesheet">
    <link href="css/common.css" rel="stylesheet">
    <link href="css/vendor/jquery.jscrollpane.css" rel="stylesheet">
    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <![endif]-->
</head><!--/head-->
<script id="item-tmpl" type="text/x-jquery-tmpl">
    {{each items}}
        <li>
            <img class="item-main-photo" src="${mainPictureLink}" />
            <div class="label">${price} AZN</div>
            {{if isNew == true}}
                <img class="label-new" src="images/icons/new.png" />
            {{/if}}
            <div class="overlay">
                <div class="author">
                    <a target="_blank" href="${authorSocialLink}" class="ava">
                        <img src="${authorAvaLink}">
                    </a>
                    <a target="_blank" href="${authorSocialLink}" class="name">${authorName}</a>
                </div>
                <div class="clear"></div>
                <div class="phone">
                    <img src="images/icons/phone-light.png" /> <span>${authorPhone}</span>
                </div>
                <div class="clear"></div>
                <div class="description">
                    <p>${description}</p>
                </div>
            </div>
            <input id="item-${id}" type="hidden" value="${id}" />
            <input class="item-index" type="hidden" value="${$index}" />
        </li>
    {{/each}}
</script>
<script id="slider-tmpl" type="text/x-jquery-tmpl">
    <div id="galleria">
        {{each otherPictures}}
            <a href="${this}">
                <img src="${this}",
                     data-big="${this}"
                     data-title="${price} AZN"
                     data-description="${description}" />
            </a>
        {{/each}}
    </div>
    <div id="item-info">
        {{if isNew == true}}
            <img class="label-new" src="images/icons/new.png" />
        {{/if}}
        <div class="left-info">
            <a target="_blank" href="${authorSocialLink}" class="ava">
                <img src="${authorAvaLink}" />
            </a>
        </div>
        <div class="middle-info">
            <div class="user-info-zone">
                <a target="_blank" href="${authorSocialLink}" class="name">
                    {{if authorSocialLink.toLowerCase().indexOf("fb") >= 0 || authorSocialLink.toLowerCase().indexOf("facebook") >= 0}}
                        <img src="images/icons/fb.png" />
                    {{else authorSocialLink.toLowerCase().indexOf("google.com") >= 0}}
                        <img src="images/icons/g.png" />
                    {{/if}}
                    <span>${authorName}</span>
                </a>
                <div class="clear"></div>
                <div class="phone">
                    <img src="images/icons/phone.png" /> <span>${authorPhone}</span>
                </div>
            </div>
            <div class="clear"></div>
            <div class="description">
                ${description}
            </div>
            <div class="clear"></div>
            <div class="date-time">
                ${dateTime}
            </div>
        </div>
        <div class="right-info">
            <div class="price">
                <span style="font-weight: bold">${price}</span> AZN
            </div>
        </div>
    </div>
</script>
<body>
<div id="header">
    <div class="content">
        <div id="logo"><a href="index.html"><img style="padding: 0 !important" src="images/logo-seller.png" alt="seller-logo" /></a></div>
        <ul>
            <li><a href="about.html">Bizim haqqımızda</a></li>
            <li><a href="contact.html">Əlaqə</a></li>
        </ul>
        <div class="clear"></div>
    </div>
</div>
<div id="content">
    <div class="left-side">
        <img class="ava" src="http://lh3.googleusercontent.com/-KK2Hpd79xqQ/AAAAAAAAAAI/AAAAAAAAAQk/k4RLXZRVs5M/s120-c/photo.jpg" />
        <div class="user-info-zone">
            <a target="_blank" href="https://www.facebook.com/amrastanov" class="name">
                <img src="images/icons/fb.png" />
                <span>${userFullName}</span>
            </a>
            <div class="clear"></div>
            <div class="phone">
                <img src="images/icons/phone.png" /> <span>${userPhone}</span>
            </div>
        </div>
    </div>
    <div class="right-side">
        <div class="content">
            <ul>
            </ul>
        </div>
    </div>
</div>
<div id="fon"></div>
<div id="slider">
    <div class="content">
    </div>
</div>

<script src="https://code.jquery.com/jquery-1.11.3.min.js"></script>
<script src="http://ajax.microsoft.com/ajax/jquery.templates/beta1/jquery.tmpl.min.js"></script>
<script src="js/vendor/galleria/galleria-1.4.2.min.js"></script>
<script src="js/vendor/jquery.jscrollpane.min.js"></script>
<script src="js/vendor/jquery.mousewheel.js"></script>
<script src="js/search.js"></script>
<script src="js/items.js"></script>
<script src="js/slider.js"></script>
<script src="js/main.js"></script>
<script>Main.initItems()</script>
</body>
</html>